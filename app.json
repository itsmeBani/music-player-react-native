{
  "expo": {
    "name": "music-player",
    "slug": "music-player",
    "version": "1.0.0",
    "scheme": "com.jiovani.musicplayer",
    "orientation": "portrait",
    "icon": "./assets/LOGOAPP.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/LOGOAPP.png",
      "resizeMode": "contain",
      "backgroundColor": "#191919"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LOGOICON.png",
        "backgroundColor": "#191919"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.jiovani.musicplayer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#191919"
    },
    "extra": {
      "eas": {
        "projectId": "65cf6bb2-5684-490d-a2b8-48e38f4a7e31"
      }
    },
    "plugins": [
      "expo-font",
      "expo-web-browser"

    ]
  }
}
